import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        double a = sc.nextDouble();
        double b = sc.nextDouble();
        sc.close();
        if(0<a && b<10){  //문제 조건
            System.out.println(a / b);
        }
    }
}
