import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int[] s = new int[5];
		int sum = 0;
		
		for (int i=0; i<5; i++) {
			s[i] = sc.nextInt();
			if (s[i]<40) s[i]=40;
			sum += s[i];
			//System.out.println(s[i]);
		}
		
		System.out.println(sum/5);
		
	}

}
